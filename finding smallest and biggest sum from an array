
#include <stdio.h>

int main ()
{
  int a[5],i,j,n,sum=0, gtreatest=0,smallest=10000;
  
  for (i = 0; i < 5; i++)
  {
      scanf("%d",&a[i]);
  }
  
  for (i = 0; i < 5; i++)
  {
      sum = 0;
      for (j = 0; j < 5; j++)
        {
            if(j == i)
            {
                sum= sum + 0;
            } 
            else
            {
                sum = sum + a[j];
            }
        }
        
        if(sum>=gtreatest)
            {
                gtreatest=sum;
            }
            
            
        else if(sum <= smallest){
                smallest=sum;
            }
            
        else{
                
            }
  
  }
  
  
  printf("%d %d",smallest, gtreatest);

}
